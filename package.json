{
  "name": "chlu-api-client",
  "version": "0.3.0",
  "description": "client to connect to the Chlu APIs",
  "main": "src/index.js",
  "repository": "https://github.com/ChluNetwork/chlu-api-client-js.git",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.6.0"
  },
  "license": "MIT",
  "author": {
    "name": "Chlu",
    "email": "info@chlu.io",
    "url": "https://chlu.io"
  },
  "bugs": {
    "url": "https://github.com/ChluNetwork/chlu-api-client-js/issues"
  },
  "homepage": "https://github.com/ChluNetwork/chlu-api-client-js/blob/master/README.md",
  "scripts": {
    "test": "mocha ./tests/**/*.test.js --exit"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chlu-ipfs-support": "ChluNetwork/chlu-ipfs-support#semver:~0.2.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "mocha": "^5.2.0",
    "sinon": "^6.1.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "no-console": 0,
      "no-debugger": 1,
      "no-var": 1,
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "no-underscore-dangle": 0,
      "no-alert": 0,
      "no-lone-blocks": 0
    },
    "globals": {}
  }
}
