{
  "name": "chlu-api-client",
  "version": "0.1.0",
  "description": "client to connect to the Chlu API",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "test": "mocha ./tests/**/*.test.js --exit"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chlu-ipfs-support": "ChluNetwork/chlu-ipfs-support#cleanup-and-fixes"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "mocha": "^5.2.0",
    "sinon": "^6.1.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "no-console": 0,
      "no-debugger": 1,
      "no-var": 1,
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "no-underscore-dangle": 0,
      "no-alert": 0,
      "no-lone-blocks": 0
    },
    "globals": {}
  }
}
